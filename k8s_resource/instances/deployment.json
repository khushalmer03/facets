{
  "kind": "k8s_resource",
  "metadata": {},
  "flavor": "k8s",
  "version": "0.2",
  "disabled": false,
  "spec": {
    "resource": {
      "apiVersion": "v1",
      "kind": "Pod",
      "metadata": {
        "name": "nginx",
        "namespace": "facets",
        "annotations": {
          "service.beta.kubernetes.io/aws-load-balancer-scheme": "internal",
          "service.beta.kubernetes.io/aws-load-balancer-type": "nlb"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "nginx",
            "image": "nginx:1.14.2",
            "ports": [
              {
                "containerPort": 80
              }
            ]
          }
        ],
        "selector": {
          "app": "facetsapi"
        },
        "type": "ClusterIP",
        "ports": [
          {
            "port": 80,
            "name": "http",
            "targetPort": 80
          }
        ]
      }
    }
  }
}