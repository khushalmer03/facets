{
  "flavor": "k8s",
  "metadata": {
    "labels": {
      "deliveryType": "MANUAL"
    }
  },
  "kind": "service",
  "disabled": false,
  "version": "0.1",
  "spec": {
    "type": "application",
    "restart_policy": "Always",
    "enable_host_anti_affinity": false,
    "runtime": {
      "ports": {
        "ports1": {
          "port": "80",
          "service_port": "80",
          "protocol": "tcp"
        }
      },
      "size": {
        "cpu": "300m",
        "memory": "1Mi",
        "cpu_limit": "1000m",
        "memory_limit": "1Mi"
      },
      "health_checks": {
        "readiness_check_type": "PortCheck",
        "readiness_start_up_time": 15,
        "readiness_timeout": 10,
        "readiness_period": 30,
        "readiness_port": "8080",
        "liveness_check_type": "PortCheck",
        "liveness_start_up_time": 15,
        "liveness_timeout": 10,
        "liveness_period": 30,
        "liveness_port": "8080"
      },
      "autoscaling": {
        "min": 1,
        "max": 1,
        "scaling_on": "CPU",
        "cpu_threshold": "50"
      },
      "volumes": {}
    },
    "release": {
      "strategy": {
        "type": "RollingUpdate"
      },
      "image": "${blueprint.self.artifacts.test-2-4511619960-nginx}"
    },
    "env": {
      "LOG_LEVEL": "INFO"
    }
  }
}